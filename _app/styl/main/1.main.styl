#main
	padding 0
	text-align center
	overflow hidden
	.container
		position relative
		z-index 20
.main-title
	font-size: 36px
	font-family: A
	font-weight: bold
.main-subtitle
	font-size: 28px
	margin 30px auto 50px
	width 100%
	max-width 500px
.main-img
	position absolute
	height 500px
	width auto
	z-index -1
	&-yulya
		left -18%
		bottom 10px
	&-andrey
		right -13%
		bottom 25px
.form
	overflow hidden
	width 100%
	max-width 730px
	margin 0 auto
	&-title
		font-size: 26px
		font-family: A
		font-weight: bold
		margin-bottom: 30px
.input
	width 100%
	max-width 350px
	border: 1px solid #000000;
	padding 19px 10px
	color #000
	font-family: A
	display block
	margin 0 auto 10px
.input-mail-main
	float left
@-webkit-keyframes autofill
	to
		font-size: 18px
		color: #000
		background-color: #fff
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.alert-message {
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	text-align: center
	font-family: A
	font-weight: bold
	line-height: 1
}
.checking {
	display: table;
	width 100%
	max-width 515px
	margin: 5px auto 0
	text-align: left
}

.checking .check-label {
	line-height: 40px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 40px;
	text-align: left;
	font-size: 13px
	font-family: R
	color #8e8d8d
}

.checking .check-label a
	text-decoration: none
	border-bottom: 1px dashed
	display: inline
	color #8e8d8d
	&:focus
		border-bottom: 1px dashed
		color #8e8d8d
	&:hover
		border-bottom: none
		text-decoration: none
		color #8e8d8d

.checking .check-label:before {
	content: '';
	background-color: #8e8d8d
	position: absolute;
	left: 0;
	top: 3px;
	width: 31px;
	height: 31px;
	border none
}

.checking .check-label:after {
	content: '';
	position: absolute;
	left: 10px;
	top: -10px;
	width: 11px;
	height: 18px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #fff
	border-right: 2px solid #fff
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + .check-label:before {
	background-color: #22334E

}

.check:checked + .check-label:after {
	opacity: 1;
	top: 7px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}