#last
	position relative
	padding 100px 0 0
	text-align center
	overflow hidden
	&:before
		content: ''
		display block
		position absolute
		width 550px
		height 550px
		transform: rotate(45deg)
		background-color: rgba(0, 85, 249, 0.3);
		bottom: -200px
		right -200px
	&:after
		content: ''
		display block
		position absolute
		width 160px
		height 160px
		transform: rotate(45deg)
		background-color: rgba(104, 156, 255, 0.2);
		bottom: -100px
		left 200px
	.container
		position relative
		z-index 32
.last-title
	font-size: 40px
	font-family: A
	font-weight: bold
	text-transform uppercase
.last-text
	font-size: 30px
	text-transform uppercase
	margin 40px 0
.button-last
	margin-bottom: 100px