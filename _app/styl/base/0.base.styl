*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: R
	background-color #fff
	color #000000
	line-height: 1.4
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	z-index 99
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.general-title
	font-size: 26px
	font-family: A
	font-weight: bold
	margin-bottom: 40px
	&-center
		text-align center
.button
	display block
	text-align center
	margin 0 auto
	background: #0055F9;
	border: 1px solid #E5E5E5;
	color #fff
	font-size: 18px
	font-family: A
	font-weight: bold
	padding 20px 10px
	width 100%
	max-width 350px
	transition: all .4s ease
	text-decoration none
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #000
		background-color: #0055F9;
		box-shadow: inset 0 0 0 50px #E5E5E5;