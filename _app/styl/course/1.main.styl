#main
	padding 0 0 60px
	text-align center
	overflow hidden
	text-align center
.pre
	font-size: 18px
	font-family: R
.main-title
	font-size: 42px
	font-family: A
	font-weight: bold
	position relative
	width 100%
	max-width 480px
	margin 20px auto 40px
	small
		display block
		font-size: 20px
		font-family: A
		font-weight: normal
		position relative
		&:before
		&:after
			content ''
			display block
			position absolute
			top 50px
			left -100px
			transform: rotate(45deg)
			width 70px
			height 70px
			background-color: #689CFF
			z-index 30
		&:after
			left auto
			right -100px
	&:before
	&:after
		content ''
		display block
		position absolute
		top -10px
		left -290px
		transform: rotate(45deg)
		width 200px
		height 200px
		background-color: #0055F9
	&:after
		left auto
		right -290px
.main-info
	font-size: 20px
	span
		display inline-block
		font-family: A
		font-weight: bold
	&:last-of-type
		margin 15px 0 50px